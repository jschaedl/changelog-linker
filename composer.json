{
    "name": "symplify/changelog-linker",
    "description": "Generates beautiful CHANGELOG.md with links to PRs, versions and users grouped in Added/Changed/Fixed/Removed categories.",
    "license": "MIT",
    "bin": [
        "bin/changelog-linker"
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.5",
        "nette/utils": "^3.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/process": "^4.4|^5.0",
        "symplify/package-builder": "^7.4",
        "symplify/set-config-resolver": "^7.4",
        "symplify/smart-file-system": "^7.4",
        "symplify/autowire-array-parameter": "^7.4",
        "symplify/auto-bind-parameter": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\ChangelogLinker\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
